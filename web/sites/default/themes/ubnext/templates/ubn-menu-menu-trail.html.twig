{# slate
  name = UBN menu: main-menu (menu-trail)
  models[links] = item
#}
<div class="ubn-menu-menu-trail">
    <ul>
      <li><a href="{{ url('<front>') }}">{{ t('Home') }}</a></li>
      {% for link in links %}
        {% if not loop.last %}
          <li><a href="{{ link.href }}">{{ link.title }}</a></li>
        {% else %}
          <li>{{ link.title }}</li>
        {% endif %}
      {% endfor %}
    </ul>
</div>
