name = UBNext
core = 7.x

; CSS
stylesheets[all][] = css/ubnext.css

; Javascripts
scripts[] = js/vendor/jquery.matchHeight-min.js
scripts[] = js/vendor/jquery.fitvids.js
scripts[] = js/vendor/jquery.cookie.js
scripts[] = js/main.js
scripts[] = js/library.js
scripts[] = js/vendor/bootstrap.min.js
scripts[] = js/vendor/history.js/scripts/bundled/html4+html5/jquery.history.js

; Panels
api[panels][styles][version] = 3
plugins[panels][layouts] = layouts
plugins[panels][styles] = styles

; Breakpoints
breakpoints[large] = (min-width: 1200px)
breakpoints[medium] = (min-width: 992px)
breakpoints[small] = (min-width: 750px)
breakpoints[extra_small] = (min-width: 0px)

; multipliers[extra_small][] = 1.5x
; multipliers[extra_small][] = 2x
